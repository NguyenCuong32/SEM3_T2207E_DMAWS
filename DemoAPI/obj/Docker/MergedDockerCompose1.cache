services:
  demoapi:
    build:
      context: /Users/tranthimaihien/Documents/fpt/SEM3/Azure/DemoAPI
      dockerfile: DemoAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: DemoAPI
      target: base
    container_name: DemoAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_URLS: https://+:443;http://+:80
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: demoapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net7.0/DemoAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    - target: 443
    tty: true
    volumes:
    - /Users/tranthimaihien/Documents/fpt/SEM3/Azure/DemoAPI/DemoAPI:/app:rw
    - /Users/tranthimaihien/vsdbg/vs2017u5:/remote_debugger:rw
    - /Users/tranthimaihien/.aspnet/https:/root/.aspnet/https:ro
    - /Users/tranthimaihien/.microsoft/usersecrets:/root/.microsoft/usersecrets:ro
    - /Users/tranthimaihien/.nuget/packages:/root/.nuget/packages:ro
    - /Users/tranthimaihien/Documents/fpt/SEM3/Azure/DemoAPI:/src:rw
version: '3.4'